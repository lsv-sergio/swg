@model int[,]

@{ 
    var range = Model.GetUpperBound(0) + 1;
    var tableName = ViewBag.TableName.ToString();
}
<table class="table" data-table-range="@range">
    <thead>
        <tr>
            <th>#</th>
            @for (int i = 0; i < Model.GetUpperBound(1) + 1; i++) {
                <th>@(i + 1)</th>
            }
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.GetUpperBound(0) + 1; i++) {
            <tr>
                <th scope="row">@(i + 1)</th>
                @for (int j = 0; j < Model.GetUpperBound(1) + 1; j++) {
                    <td><span name="@{@tableName}[@i,@j]">@Model[i, j]</span></td>
                }
            </tr>
        }
    </tbody>
</table>
